{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/get-lists",
            "destination": "/api/get-lists"
        },
        {
            "source": "/register",
            "destination": "/api/register"
        },
        {
            "source": "/login",
            "destination": "/api/login"
        }
    ]
}
